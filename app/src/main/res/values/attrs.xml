<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ColorTrackTextView">
        <attr name="originColor" format="color"/>
        <attr name="changeColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="TrackIndicatorView">
        <attr name="visibleNums" format="integer"/>
    </declare-styleable>

    <declare-styleable name="BannerView">
        <attr name="bvDotFocus" format="color|reference"/>
        <attr name="bvDotNormal" format="color|reference"/>
        <attr name="bvDotWidth" format="dimension"/>
        <attr name="bvDotMargin" format="dimension"/>
        <attr name="bvDotIndicatorGravity" format="enum">
            <enum name="center" value="0"/>
            <enum name="left" value="1"/>
            <enum name="right" value="2"/>
        </attr>
        <attr name="bvBottomColor" format="color|reference"/>
        <attr name="bvWidthProportion" format="float"/>
        <attr name="bvHeightProportion" format="float"/>
    </declare-styleable>

    <declare-styleable name="StepView">
        <attr name="outColor" format="reference|color"/>
        <attr name="innerColor" format="color|reference"/>
        <attr name="borderWidth" format="dimension"/>
        <attr name="stepTextColor" format="color|reference"/>
        <attr name="stepTextSize" format="dimension"/>
    </declare-styleable>
    
    <declare-styleable name="RoundProgressBar">
        <attr name="innerBackground" format="color"/>
        <attr name="outerBackground" format="color"/>
        <attr name="roundWidth" format="dimension"/>
        <attr name="progressTextSize" format="dimension"/>
        <attr name="progressTextColor" format="color"/>
    </declare-styleable>
    
    <declare-styleable name="RatingBarView">
        <attr name="maxRating" format="integer"/>
        <attr name="normalStar" format="reference"/>
        <attr name="focusStar" format="reference"/>
    </declare-styleable>

    <declare-styleable name="LetterSideBar">
        <attr name="lsTextSize" format="dimension"/>
        <attr name="lsTextColorNormal" format="color"/>
        <attr name="lsTextColorFocus" format="color"/>
    </declare-styleable>

    <declare-styleable name="KGSlidingMenu">
        <attr name="slidingRightMargin" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="QQSlidingMenu">
        <attr name="slidingQQRightMargin" format="dimension"/>
    </declare-styleable>
</resources>
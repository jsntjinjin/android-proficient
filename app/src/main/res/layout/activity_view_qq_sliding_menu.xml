<?xml version="1.0" encoding="utf-8"?>
<com.fastaoe.proficient.component.views.SlidingMenu.QQSlidingMenu
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:slidingQQRightMargin="60dp"
    android:overScrollMode="never"
    android:id="@+id/kg_slding_menu"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">
        <include layout="@layout/item_slidng_menu_menu"/>

        <include
            android:id="@+id/content"
            layout="@layout/item_slidng_menu_content"/>
    </LinearLayout>

</com.fastaoe.proficient.component.views.SlidingMenu.QQSlidingMenu>